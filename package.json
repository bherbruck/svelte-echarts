{
  "name": "svelte-echarts",
  "version": "1.0.0-rc3",
  "license": "MIT",
  "description": "Svelte component for Apache ECharts",
  "author": {
    "name": "Brennen Herbruck",
    "url": "https://github.com/bherbruck"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bherbruck/svelte-echarts.git"
  },
  "scripts": {
    "dev": "svelte-kit sync && vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package -i ./src/lib/svelte-echarts && publint",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest run",
    "lint": "prettier --check .",
    "format": "prettier --write .",
    "test:ui": "vitest --ui",
    "test:watch": "vitest"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "peerDependencies": {
    "echarts": "^5.0.0",
    "svelte": ">=5"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^4.0.0",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.17.3",
    "@sveltejs/package": "^2.3.10",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@testing-library/svelte": "^5.2.7",
    "@vitest/browser": "^3.0.7",
    "@vitest/coverage-v8": "^3.0.7",
    "@vitest/ui": "^3.0.7",
    "echarts": "^5.6.0",
    "jsdom": "^26.0.0",
    "playwright": "^1.50.1",
    "prettier": "^3.5.2",
    "prettier-plugin-svelte": "^3.3.3",
    "publint": "^0.3.6",
    "svelte": "^5.20.5",
    "svelte-check": "^4.1.4",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3",
    "vite": "^6.2.0",
    "vitest": "^3.0.7",
    "vitest-browser-svelte": "^0.1.0"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "packageManager": "pnpm@10.5.2+sha512.da9dc28cd3ff40d0592188235ab25d3202add8a207afbedc682220e4a0029ffbff4562102b9e6e46b4e3f9e8bd53e6d05de48544b0c57d4b0179e22c76d1199b"
}
